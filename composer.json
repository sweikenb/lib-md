{
    "name": "sweikenb/lib-md",
    "description": "Markdown library",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Sweikenb\\Library\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
